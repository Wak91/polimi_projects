package it.polimi.ingegneriaDelSoftware2013.horseFever_lorenzo2.fontana_fabio1.gritti;


public class Scommessa {
	
	private final int denaro;
	private final String colore; // facciamo un enum? 
	private final String tipo; // piazzata o vincente

	
	public Scommessa(int money,String color,String type) {
		this.denaro=money;
		this.colore=color;
		this.tipo=type;
		
	}
	
	@Override
	public String toString() {
		return("\n-->Denaro: "+this.denaro+"\n-->Colore: "+this.colore+"\n-->Tipo: "+this.tipo);
	}

}
