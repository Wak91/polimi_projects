package Gui_Events;

import Controller_events.HorseFeverEventController;


public class setScommessaEvent extends HorseFeverEventGui {

	int money;
	String tipo;
	String cavallo;
	
	public setScommessaEvent(int soldi,String type,String horse) {
		super(setScommessaEvent.class);
		money=soldi;
		tipo=type;
		cavallo=horse;
	}

	public int getMoney() {
		return money;
	}


	public String getTipo() {
		return tipo;
	}


	public String getCavallo() {
		return cavallo;
	}
	
}
